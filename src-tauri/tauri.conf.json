{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "margin",
  "version": "0.1.0",
  "identifier": "org.margin.desktop",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../build"
  },
"app": {
  "windows": [
    {
      "label": "main",
      "title": "margin-desktop",
      "width": 800,
      "height": 600,
      "url": "http://localhost:1420"
    },
    {
      "label": "secondary",
      "title": "Secondary Window",
      "width": 600,
      "height": 400,
      "url": "http://localhost:1420"
    }
  ],
    "security": {
      "csp": null
    }
},
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
